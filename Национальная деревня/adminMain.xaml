        <Button x:Name="order_page" Content="Заказы" HorizontalAlignment="Left" Margin="121,238,0,0" VerticalAlignment="Top" Width="117" Click="order_page_Click" Background="#FFB8C384" Height="25" FontFamily="Segoe UI Semibold" FontSize="16" BorderBrush="{x:Null}"/>
        <Button x:Name="event_page" Content="Мероприятия" HorizontalAlignment="Left" Margin="121,151,0,0" VerticalAlignment="Top" Width="117" Click="event_page_Click" Background="#FFB8C384" Height="25" FontFamily="Segoe UI Semibold" FontSize="16" BorderBrush="{x:Null}"/>

    </Grid>
</Window>
